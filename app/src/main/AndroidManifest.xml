<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nabee.gymsocialmedia">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".logIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".profilePage.profile" />
        <activity android:name=".excerciseLogging.logHome" />
        <activity android:name=".Social.socialFeed" />
        <activity android:name=".excerciseLogging.detailOverview" />
        <activity
            android:name=".excerciseLogging.exerciseForm"
            android:label="@string/title_activity_exercise_form"
            android:theme="@style/AppTheme"
            android:exported="true"></activity>
    </application>

</manifest>